<launch>

    <arg name="rviz" default="false"/>

    <group ns="ona2">
        <node pkg="fast_limo" type="fast_limo_reloca_exec" name="fast_limo_reloca" output="screen">
            <rosparam file="$(find fast_limo)/config/relocator.yaml" subst_value="True"/>
        </node>
    </group>

    <node pkg="rviz" name="rviz_limo" type="rviz" args="-d $(find fast_limo)/config/rviz/limo.rviz" if="$(arg rviz)"/>


</launch>
